<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>{{htmlWebpackPlugin.options.title}}</title>
    <link rel="stylesheet" type="text/css" href="./src/index.css" >
    <script src="http://vk.com/js/api/openapi.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.10/handlebars.min.js" type="text/javascript"></script>
</head>
<body>
<section>
    <div class="window">
        <div class="container">
            <div class="box">
                <h1 id="headerInfo"></h1>
            </div>
            <div class="box__subheader" id="results">
                <form action="/search" target="_parent" method="get" class="search__block">
                    <input name="search" value="" type="text" placeholder="Начните вводить имя друга" class="search__line">
                    <input name="search" value="" type="text" placeholder="Название" class="name__line">
                </form>
            </div>
            <div class="friends">
                <div class="friends__get">
                    <div class="get__title">Ваши друзья</div>
                    <script id="user-template" type="text/x-handlebars-template">
                        <ul class="get__list" id="list">

                        </ul>
                    </script>
                </div>
                <div class="friends__picked">
                    <div class="picked__title">Друзья в списке</div>
                    <ul class="picked__list">

                    </ul>
                </div>
            </div>
            <div class="footer">
                <div class="btn">Сохранить</div>
            </div>
        </div>
    </div>
</section>
<script src="src/ff.js" type="text/javascript"></script>
</body>
</html>
